{
  "family": "application-api-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::750934177202:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "application-api",
      "image": "REPLACE_IMAGE",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
